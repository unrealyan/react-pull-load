{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "outDir": "",
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": false,
//    "noEmit": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "custom.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
